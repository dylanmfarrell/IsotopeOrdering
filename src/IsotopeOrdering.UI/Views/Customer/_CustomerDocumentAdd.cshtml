@model CustomerDocumentDetailModel
<li class="list-group-item add-collection-item">
    <div class="form-row">
        <div class="form-group col-6">
            <label asp-for="ExpirationDate">Expiration Date</label>
            <input asp-for="ExpirationDate" class="form-control form-control-sm" type="date" />
        </div>
        <partial name="_Document" model="new DocumentDetailModel()" view-data="new ViewDataDictionary(ViewData) { { ViewDataKeys.UploadModule,UploadModules.CustomerDocument} }" />
    </div>
    <button type="button" class="btn btn-sm btn-success mt-5" onclick="addCustomerDocument(this)" data-url="@Url.Action(nameof(CustomerController.AddCustomerDocument))" data-target="#customer-document-list">Add New Document</button>
</li>
