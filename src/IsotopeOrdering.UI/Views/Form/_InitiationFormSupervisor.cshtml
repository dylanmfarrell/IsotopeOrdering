@model FormInitiationSignatureModel
@if (ViewData.IsReadonly()) {
    <div class="form-group row">
        <label asp-for="PrintedName" class="col-sm-3 col-form-label">Supervisor Name</label>
        <div class="col-sm-9">
            <input type="text" class="form-control form-control-sm" readonly asp-for="PrintedName">
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Email" class="col-sm-3 col-form-label">Supervisor Contact Email</label>
        <div class="col-sm-9">
            <input type="text" class="form-control form-control-sm" readonly asp-for="Email">
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="PhoneNumber" class="col-sm-3 col-form-label">Supervisor Contact Phone Number</label>
        <div class="col-sm-9">
            <input type="text" class="form-control form-control-sm" readonly asp-for="PhoneNumber">
        </div>
    </div>
}
else {
    <div class="form-group row">
        <label asp-for="PrintedName" class="col-sm-3 col-form-label">Supervisor Name</label>
        <div class="col-sm-9">
            <input type="text" class="form-control form-control-sm" asp-for="PrintedName">
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Email" class="col-sm-3 col-form-label">Supervisor Email Address</label>
        <div class="col-sm-9">
            <input type="text" class="form-control form-control-sm" asp-for="Email">
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="PhoneNumber" class="col-sm-3 col-form-label">Supervisor Phone Number</label>
        <div class="col-sm-9">
            <input type="text" class="form-control form-control-sm" asp-for="PhoneNumber">
        </div>
    </div>
}