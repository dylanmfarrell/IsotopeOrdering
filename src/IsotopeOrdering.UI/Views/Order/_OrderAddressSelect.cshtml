@model List<OrderAddressDetailModel>
@{
    AddressType type = (AddressType)ViewData[ViewDataKeys.AddressType];
}
<ul class="list-group" name="order-address-list">
    @foreach (var address in Model) {
        <partial name="_OrderAddressView" model="address" />
    }
    <partial name="_OrderAddressAdd" model="new OrderAddressDetailModel() { Type = type }" />
</ul>
